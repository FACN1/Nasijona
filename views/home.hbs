<header class="landing-header">
  <div class="slash"></div>
  <h1 class="header-text">NAZBAY</h1>
  <div class="login-button-wrap">
    <a class="account-button" id="login-button">{{loginStatus}}</a>
    {{#if user}}
      <a class="account-button" href="/profile?u={{user}}">MY PROFILE</a>
    {{else}}
      <a class="account-button" href="/register">REGISTER</a>
    {{/if}}
  </div>
</header>

<section class="landing-section">
  <img class="logo" src="./assets/logo.svg" alt="NazBay Logo">
</section>

<script type="text/javascript" src="./scripts/home.js"></script>
